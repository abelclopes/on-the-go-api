<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<package xmlns="http://www.zend.com/server/deployment-descriptor/1.0" version="1.0">
    <name>on-the-go-api</name>
    <version>
        <release>0.1</release>
    </version>
    <appdir>data</appdir>
    <docroot>data/public</docroot>
    <scriptsdir>scripts</scriptsdir>
    <parameters>
        <parameter id="API_KEY" display="WebAPI Key" required="true" type="string" readonly="false"/>
        <parameter id="API_HASH" display="WebAPI Hash" required="true" type="string" readonly="false"/>
        <parameter id="OAUTH2_DB_USER" display="OAuth2 Database User" required="true" type="string" readonly="false"/>
        <parameter id="OAUTH2_DB_PASSWORD" display="OAuth2 Database Password" required="true" type="password" readonly="false"/>
        <parameter id="OAUTH2_DB_NAME" display="OAuth2 Database Name" required="true" type="string" readonly="false"/>
    </parameters>
</package>
