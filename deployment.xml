<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<package xmlns="http://www.zend.com/server/deployment-descriptor/1.0" version="1.0">
    <name>on-the-go-api</name>
    <version>
        <release>0.1</release>
    </version>
    <appdir>data</appdir>
    <docroot>data/public</docroot>
    <scriptsdir>scripts</scriptsdir>
    <parameters>
        <parameter display="WebAPI Key" id="API_KEY" readonly="false" required="true" type="string"/>
        <parameter display="WebAPI Hash" id="API_HASH" readonly="false" required="true" type="string"/>
        <parameter display="OAuth2 Database User" id="OAUTH2_DB_USER" readonly="false" required="true" type="string"/>
        <parameter display="OAuth2 Database Password" id="OAUTH2_DB_PASSWORD" readonly="false" required="true" type="password"/>
        <parameter display="OAuth2 Database Name" id="OAUTH2_DB_NAME" readonly="false" required="true" type="string"/>
        </parameters>
    </package>
